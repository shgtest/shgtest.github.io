define("recommend.comment_number",["F"],function(e){var d=null,g=null,b="http://comment.ifeng.com/get.php",a="",f=null,h="";var c=function(){f=setInterval(function(){if(e.query("#recommend div.js_recommendList").length>0){clearInterval(f);d=e.find(e.query("#recommend div.js_recommendList")[0],"span.s02");g=e.find(e.find(e.query("#recommend div.js_recommendList")[0],"h2"),"a")[0];if("undefined"!==typeof g&&g.href){a=(g.href).split("#")[0].split("?")[0]}else{return false}h="http://gentie.ifeng.com/view.html?docUrl="+encodeURIComponent(a)+"&docName="+encodeURIComponent(e.trim(g.innerHTML));e.ajax({url:b,dataType:"jsonp",data:{format:"js",job:3,docurl:a},cache:true,callback:"callbackRecommendCommentCount",success:function(i){e.html(e.find(d,"em"),i)}})}},200)};return c});define("comment.number",["F"],function(f){var e=null,c=null,b=null,a=null,h="",d="";var g="http://comment.ifeng.com/get.php";return function(i){h=encodeURIComponent(i.docUrl);d=encodeURIComponent(i.docName);if("undefined"===typeof window.PEOPLE_NUMBER||"undefined"===typeof window.COMMENT_NUMBER){f.ajax({url:g,dataType:"jsonp",data:{doc_url:i.docUrl,format:"js",job:14,callback:"callbackGetFastCommentCount"},cache:true,callback:"callbackGetFastCommentCount",success:function(j){var n="",m="";try{if("undefined"!==typeof i.skey){n="&skey="+i.skey}if("undefined"!==typeof i.speUrl){m="&speUrl="+encodeURIComponent(i.speUrl)}var l="http://gentie.ifeng.com/view.html?docUrl="+h+"&docName="+d+n+m;window.PEOPLE_NUMBER=j.join_count/1;window.COMMENT_NUMBER=j.count/1;b=setInterval(function(){e=f.query(".js_peopleNumber");if(e.length>0){for(var p=0,o=e.length;p<o;p++){if(e[p].tagName==="A"){f.attr(e[p],"href",l)}f.html(f.query("em",e[p]),window.PEOPLE_NUMBER)}clearInterval(b)}},100);a=setInterval(function(){c=f.query(".js_commentNumber");if(c.length>0){for(var o=0,p=c.length;o<p;o++){if(c[o].tagName==="A"){f.attr(c[o],"href",l)}f.html(f.query("em",c[o]),window.COMMENT_NUMBER)}clearInterval(a)}},100)}catch(k){}}})}}});
